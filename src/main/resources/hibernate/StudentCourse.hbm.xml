<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.StudentCourse" table="student_course" schema="homeworksystem">
        <composite-id mapped="true" class="com.bupt.se.homework.entity.StudentCoursePK">
            <key-property name="studentId" column="STUDENT_ID"/>
            <key-property name="courseId" column="COURSE_ID"/>
        </composite-id>
        <property name="grade" column="GRADE"/>
    </class>
</hibernate-mapping>