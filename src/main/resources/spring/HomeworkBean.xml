<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="homeworkBo" class="com.bupt.se.homework.bo.impl.HomeworkBoImpl">
        <property name="homeworkDAO" ref="homeworkDAO"/>
    </bean>
    <bean id="homeworkDAO" class="com.bupt.se.homework.dao.impl.HomeworkDAOImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>

    <bean id="homeworkGroupDAO" class="com.bupt.se.homework.dao.impl.HomeworkGroupDAOImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
</beans>