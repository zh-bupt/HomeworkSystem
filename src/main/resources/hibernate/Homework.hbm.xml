<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.Homework" table="homework" schema="homeworksystem">
        <id name="homeworkId" column="HOMEWORK_ID"/>
        <property name="courseId" column="COURSE_ID"/>
        <property name="content" column="CONTENT"/>
        <property name="releasetime" column="RELEASETIME"/>
        <property name="deadline" column="DEADLINE"/>
    </class>
</hibernate-mapping>