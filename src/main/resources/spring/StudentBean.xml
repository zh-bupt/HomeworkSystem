<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="studentAction" class="com.bupt.se.homework.action.StudentAction">
        <property name="studentBo" ref="studentBo"/>
        <property name="courseBo" ref="courseBo"/>
    </bean>
    <bean id="studentBo" class="com.bupt.se.homework.bo.impl.StudentBoImpl">
        <property name="studentDAO" ref="studentDAO"/>
    </bean>
    <bean id="studentDAO" class="com.bupt.se.homework.dao.impl.StudentDAOImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
</beans>