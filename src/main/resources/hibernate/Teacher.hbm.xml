<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.Teacher" table="teacher" schema="homeworksystem">
        <id name="teacherId" column="TEACHER_ID"/>
        <property name="teacherName" column="TEACHER_NAME"/>
        <property name="sex" column="SEX"/>
        <property name="profession" column="PROFESSION"/>
        <property name="telephone" column="TELEPHONE"/>
        <property name="email" column="EMAIL"/>
        <property name="password" column="PASSWORD"/>
    </class>
</hibernate-mapping>