<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.GroupCourse" table="group_course" schema="homeworksystem">
        <id name="groupId" column="GROUP_ID"/>
        <property name="courseId" column="COURSE_ID"/>
        <property name="capacity" column="CAPACITY"/>
        <property name="name" column="NAME"/>
        <property name="leaderId" column="LEADER_ID"/>
        <property name="groupScore" column="GROUP_SCORE"/>
    </class>
</hibernate-mapping>