<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.Course" table="course" schema="homeworksystem">
        <id name="courseId" column="COURSE_ID"/>
        <property name="courseName" column="COURSE_NAME"/>
        <property name="teacherId" column="TEACHER_ID"/>
        <property name="capacity" column="CAPACITY"/>
        <property name="createTime" column="CREATE_TIME"/>
    </class>
</hibernate-mapping>