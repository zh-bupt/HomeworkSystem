<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.se.homework.entity.HomeworkGroup" table="homework_group" schema="homeworksystem">
        <composite-id mapped="true" class="com.bupt.se.homework.entity.HomeworkGroupPK">
            <key-property name="groupId" column="GROUP_ID"/>
            <key-property name="homeworkId" column="HOMEWORK_ID"/>
        </composite-id>
        <property name="submissionTime" column="SUBMISSION_TIME"/>
        <property name="score" column="SCORE"/>
        <property name="comment" column="COMMENT"/>
        <property name="filedir" column="FILEDIR"/>
    </class>
</hibernate-mapping>